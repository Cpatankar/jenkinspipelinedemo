 pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
        stage('release') {
            steps {
                echo 'releasing'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
    }
}
